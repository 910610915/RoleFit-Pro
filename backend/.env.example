# Backend Environment Configuration
# Copy this file to .env and update values

# Application
APP_NAME=HardwareBenchmark
DEBUG=false
SECRET_KEY=your-secret-key-change-in-production-min-32-chars

# Database
DATABASE_URL=postgresql+asyncpg://benchmark_user:benchmark_pass_2026@postgres:5432/hardware_benchmark
DATABASE_URL_SYNC=postgresql://benchmark_user:benchmark_pass_2026@postgres:5432/hardware_benchmark

# Redis
REDIS_URL=redis://redis:6379/0

# InfluxDB
INFLUXDB_URL=http://influxdb:8086
INFLUXDB_TOKEN=my-super-secret-admin-token
INFLUXDB_ORG=hardware-benchmark
INFLUXDB_BUCKET=metrics

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost

# JWT
JWT_SECRET_KEY=your-jwt-secret-key-change-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=1440  # 24 hours
